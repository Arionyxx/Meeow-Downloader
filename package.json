{
  "name": "meeow-downloader",
  "version": "1.0.0",
  "description": "A purr-fectly designed download manager with a cozy cat theme",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "lint": "eslint .",
    "test": "vitest run",
    "format": "prettier --write .",
    "package": "electron-builder",
    "dist:linux": "npm run build && electron-builder --linux && npm run checksums",
    "dist:win": "npm run build && electron-builder --win && npm run checksums",
    "checksums": "node scripts/generate-checksums.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meow-dev/meeow-downloader.git"
  },
  "keywords": [],
  "author": "MeowDev <meow@example.com>",
  "license": "ISC",
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/webtorrent": "^0.110.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
